  "devDependencies": {
    "babel-cli": "6.2.0",
    "babel-core": "6.1.21",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-2": "6.1.18",
    "deindent": "0.1.0",
    "eslint": "1.9.0",
    "eslint-config-kentcdodds": "5.0.0",
